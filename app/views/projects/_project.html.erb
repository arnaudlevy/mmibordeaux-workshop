<%= link_to project do %>
  <article class="project thumbnail">
    <h1><%= project.name %></h1>
    <p>(<%= link_to project.workshop, project.workshop unless project.workshop.nil? %>)</p>
    <p><%= project.description %></p>
    <p><%= link_to project.url, project.url, target: :blank %></p>
  </article>
<% end %>